{
  "name": "zakhaarif-launcher",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "main-server": "vite",
    "dev": "npm run compile-credits && npm run sw && npm run main-server",
    "type-check-sw": "tsc --project ./serviceWorkers/tsconfig.json --noEmit",
    "type-check-main": "tsc --project tsconfig.json --noEmit",
    "type-check": "npm run type-check-main && npm run type-check-sw",
    "build": "npm run build-main && npm run build-sandbox",
    "build-main": "npm run compile-credits && npm run sw && vite build && node ./src/build/headers.mjs",
    "build-sandbox": "npm run sw-sandbox && node ./cli/copyFolder.mjs --source=sandbox/src --dest=dist-sandbox && node ./sandbox/build/headers.mjs",
    "sw-sandbox": "node ./sandbox/generateHtml.mjs && esbuild ./sandbox/secure.ts --bundle --sourcemap --minify --format=esm --outfile=./sandbox/src/secure.compiled.js && node ./sandbox/inlineCoreFiles.mjs && esbuild ./sandbox/serviceWorker/index.ts --bundle --minify --sourcemap --outfile=./sandbox/src/mirror-sw.compiled.js",
    "sandbox-server": "node ./sandbox/dev-server.mjs",
    "dev-sandbox": "npm run sw-sandbox && npm run sandbox-server",
    "preview": "vite build --outDir dist-preview && vite preview --outDir dist-preview",
    "sw": "npm run sw-prod && npm run sw-dev",
    "test-watch": "vitest watch",
    "test": "vitest run",
    "sw-prod": "esbuild ./serviceWorkers/index.ts --bundle --sourcemap --minify --outfile=./public/sw.compiled.js",
    "sw-dev": "esbuild ./serviceWorkers/index.ts --bundle --sourcemap --outfile=./public/dev-sw.compiled.js",
    "check-types": "tsc",
    "analyze-bundle": "npx vite-bundle-visualizer --output=bundle-anaylsis/index.html --open --template sunburst",
    "compile-credits": "node ./cli/compileCredits.mjs"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/material": "^5.10.16",
    "alea": "^1.0.1",
    "babylonjs": "^5.38.0",
    "babylonjs-loaders": "^5.38.0",
    "babylonjs-materials": "^5.38.0",
    "dexie": "^3.2.2",
    "huzma": "^0.1.0",
    "material-ui-confirm": "^3.0.7",
    "nanoid": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.5",
    "simplex-noise": "^4.0.1",
    "small-semver": "^0.1.0",
    "wasm-check": "^2.1.2",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.10",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "clean-css": "^5.3.1",
    "command-line-args": "^5.2.1",
    "cors": "^2.8.5",
    "esbuild": "^0.15.17",
    "express": "^4.18.2",
    "fast-png": "^6.1.0",
    "filehound": "^1.17.6",
    "fs-extra": "^11.1.0",
    "postcss": "^8.4.19",
    "semver": "^7.3.8",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^3.2.3",
    "vitest": "^0.25.5"
  }
}
