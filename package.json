{
  "name": "zakhaarif-launcher",
  "private": true,
  "version": "0.1.0",
  "license": "AGPL-3",
  "scripts": {
    "dev": "npm run build-lib && concurrently --kill-others -n huzem,main \"npm run dev-huzem\" \"npm run dev-main\"",
    "dev-main": "npm run compile-credits && npm run sw-dev && vite",
    "dev-huzem": "node ./huzem/devServer.mjs",
    "preview": "npm run build && vite preview --outDir dist --host",
    "build": "npm run build-lib && npm run build-main && npm run build-huzem",
    "build-main": "npm run compile-credits && npm run sw-prod && vite build && node ./cli/headers.mjs && npx huzma-create",
    "build-huzem": "npm run cache-assets && node ./cli/copyFolder.mjs --source=huzem/public/large-assets --dest=dist/large-assets && node ./huzem/build.mjs",
    "build-lib": "node ./huzem/buildDevTools.mjs",
    "sw-prod": "esbuild ./serviceWorkers/index.ts --bundle --sourcemap --minify --outfile=./public/sw.compiled.js",
    "sw-dev": "esbuild ./serviceWorkers/index.ts --bundle --sourcemap --outfile=./public/dev-sw.compiled.js",
    "compile-credits": "node ./cli/compileCredits.mjs",
    "cache-assets": "node ./cli/cacheAssets.mjs",
    "type-check": "tsc --project tsconfig.json --noEmit && tsc --project serviceWorkers/tsconfig.json --noEmit && tsc --project huzem/tsconfig.json --noEmit",
    "lint": "npx eslint .",
    "lint-fix": "npx eslint . --fix",
    "test-watch": "vitest watch",
    "test": "vitest run",
    "analyze-bundle": "npx vite-bundle-visualizer --output=bundle-anaylsis/index.html --open --template sunburst",
    "install-githooks": "npx husky install && npx husky add .husky/pre-commit \"npm run pre-commit\"",
    "pre-commit": "npm run lint-fix && git add -A"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/material": "^5.10.16",
    "dexie": "^3.2.2",
    "huzma": "^0.6.0",
    "material-ui-confirm": "^3.0.7",
    "nanoid": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.5",
    "small-semver": "^0.1.1",
    "w-worker-rpc": "^0.1.3",
    "wasm-check": "^2.1.2",
    "zakhaarif-huzem-internal-pkg": "file:huzem",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/command-line-args": "^5.2.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.11.10",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "clean-css": "^5.3.1",
    "command-line-args": "^5.2.1",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "esbuild": "^0.15.17",
    "eslint": "^8.36.0",
    "express": "^4.18.2",
    "fast-png": "^6.1.0",
    "filehound": "^1.17.6",
    "fs-extra": "^11.1.0",
    "husky": "^8.0.3",
    "node-fetch": "^3.3.0",
    "postcss": "^8.4.19",
    "semver": "^7.3.8",
    "sirv": "^2.0.2",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^3.2.3",
    "vitest": "^0.25.5",
    "zakhaarif-dev-tools": "file:huzem/src/zakhaarif-dev-tools/build"
  }
}
