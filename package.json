{
  "name": "g-wrapper",
  "private": true,
  "version": "0.1.0-beta.0",
  "type": "module",
  "scripts": {
    "main-server": "vite",
    "dev": "npm run sw && npm run main-server",
    "type-check": "tsc",
    "build": "npm run type-check && npm run build-main",
    "generate-cargo-meta": "node ./src/cli/getStandardCargosMeta.mjs",
    "build-main": "npm run generate-cargo-meta && npm run sw && vite build",
    "sw-sandbox": "node ./sandbox/generateHtml.mjs && esbuild ./sandbox/secure.ts --bundle --minify --format=esm --outfile=./sandbox/src/secure.compiled.js && node ./sandbox/inlineCoreFiles.mjs && esbuild ./sandbox/serviceWorker/index.ts --bundle --minify --outfile=./sandbox/src/sw.compiled.js",
    "sandbox-server": "node ./sandbox/dev-server.mjs",
    "dev-sandbox": "npm run sw-sandbox && npm run sandbox-server",
    "build-sandbox": "npm run sw-sandbox && node ./cli/copyFolder.mjs --source=sandbox/src --dest=dist-sandbox",
    "preview": "vite build --outDir dist-preview && vite preview --outDir dist-preview",
    "sw": "npm run sw-prod && npm run sw-dev",
    "test-watch": "vitest watch",
    "sw-prod": "esbuild ./serviceWorkers/index.ts --bundle --minify --outfile=./public/sw.compiled.js",
    "sw-dev": "esbuild ./serviceWorkers/index.ts --bundle --outfile=./public/dev-sw.compiled.js",
    "cargo-cli": "node ./src/lib/cargo/cli.mjs",
    "check-types": "tsc",
    "analyze-bundle": "npx vite-bundle-visualizer --output=bundle-anaylsis/index.html --open --template sunburst"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/icons-material": "^5.10.16",
    "@mui/material": "^5.10.16",
    "alea": "^1.0.1",
    "babylonjs": "^5.38.0",
    "babylonjs-loaders": "^5.38.0",
    "babylonjs-materials": "^5.38.0",
    "material-ui-confirm": "^3.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.5",
    "simplex-noise": "^4.0.1",
    "wasm-check": "^2.1.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.10",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "clean-css": "^5.3.1",
    "cors": "^2.8.5",
    "esbuild": "^0.15.17",
    "express": "^4.18.2",
    "fast-png": "^6.1.0",
    "filehound": "^1.17.6",
    "fs-extra": "^11.1.0",
    "nanoid": "^4.0.0",
    "postcss": "^8.4.19",
    "semver": "^7.3.8",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^3.2.3",
    "vitest": "^0.25.5",
    "zod": "^3.19.1"
  }
}
